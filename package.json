{
  "name": "@ximatriarx/typescript",
  "version": "0.0.11",
  "description": "TypeScript",
  "keywords": [
    "Xi",
    "Matriarx",
    "JavaScript",
    "TypeScript"
  ],
  "author": "Xi Matriarx",
  "license": "MIT",
  "homepage": "https://ximatriarx.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XiMatriarx/typescript.git"
  },
  "bugs": {
    "url": "https://github.com/XiMatriarx/typescript/issues"
  },
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc --project .typescriptrc.json",
    "dev": "tsc --project .typescriptrc.json --watch",
    "lint": "eslint src test",
    "style": "prettier --write 'src/**/*.{json,ts}' 'test/**/*.{json,ts}'",
    "test": "jest --config .jestrc.json",
    "docker": "docker builder build . -t typescript && docker run typescript",
    "init": "husky hooks"
  },
  "bin": {
    "init": "bin/init.sh"
  },
  "access": "public",
  "all": false,
  "allow-same-version": false,
  "audit": false,
  "audit-level": "low",
  "auth-type": "web",
  "bin-links": false,
  "browser": false,
  "call": "",
  "color": "always",
  "commit-hooks": true,
  "depth": 1,
  "diff-dst-prefix": "b/",
  "diff-ignore-all-space": false,
  "diff-name-onlyh": false,
  "diff-no-prefix": false,
  "diff-src-prefix": "a/",
  "diff-text": false,
  "diff-unified": 3,
  "dry-run": false,
  "engine-strict": false,
  "fetch-retries": 2,
  "fetch-retry-factor": 10,
  "fetch-retry-maxtimeout": 60000,
  "fetch-retry-mintimeout": 10000,
  "fetch-timeout": 300000,
  "force": false,
  "foreground-scripts": false,
  "format-package-lock": true,
  "fund": false,
  "git": "git",
  "git-tag-version": true,
  "global": false,
  "global-style": false,
  "heading": "npm",
  "if-present": false,
  "ignore-scripts": false,
  "include-staged": false,
  "include-workspace-root": false,
  "init-author-email": "xi@ximatriarx.io",
  "init-author-name": "Xi Matriarx",
  "init-author-url": "https://ximatriarx.io",
  "init-license": "MIT",
  "init-version": "0.0.1",
  "install-links": false,
  "json": false,
  "legacy-bundling": false,
  "legacy-peer-deps": false,
  "link": false,
  "location": "user",
  "lockfile-version": 3,
  "loglevel": "notice",
  "logs-max": 10,
  "long": false,
  "maxsockets": 10,
  "message": "%s",
  "offline": false,
  "omit": "dev",
  "omit-lockfile-registry-resolved": false,
  "pack-destination": ".",
  "package-lock": true,
  "package-lock-only": false,
  "parseable": false,
  "prefer-offline": false,
  "prefer-online": false,
  "preid": "",
  "progress": true,
  "read-only": false,
  "rebuild-bundle": true,
  "registry": "https://registry.npmjs.org",
  "replace-registry-host": "npmjs",
  "save": true,
  "save-bundle": false,
  "save-dev": false,
  "save-exact": false,
  "save-optional": false,
  "save-peer": false,
  "save-prefix": "^",
  "save-prod": false,
  "scope": "@XiMatriarx",
  "searchexclude": "",
  "searchlimit": 10,
  "searchopts": "",
  "searchstaleness": 600,
  "sign-git-commit": true,
  "sign-git-tag": true,
  "strict-peer-deps": false,
  "strict-ssl": true,
  "tag": "latest",
  "tag-version-prefix": "v",
  "timing": false,
  "umask": 0,
  "update-notifier": true,
  "usage": false,
  "versions": false,
  "workspace-update": true,
  "dependencies": {
    "@types/node": "^20.11.17",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2"
  }
}
